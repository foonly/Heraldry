# show php errors
php_flag display_startup_errors on
php_flag display_errors on
php_flag html_errors on

Options +FollowSymLinks
RewriteEngine On
RewriteCond %{REQUEST_METHOD} =POST
RewriteRule ^([0-9A-Za-z_]+)  /transaction.php?template=$1 [QSA]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^render/([A-Za-z_]+)([0-9]*)\.([A-Za-z_]+) /render.php?type=$1&format=$3&id=$2 [QSA]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^account(/([0-9A-Za-z_]+))? /account.php?template=$2 [QSA]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^admin(/([0-9A-Za-z_]+))? /admin.php?template=$2 [QSA]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([0-9A-Za-z]+)(_([0-9]+))?  /index.php?template=$1&id=$3 [QSA]
